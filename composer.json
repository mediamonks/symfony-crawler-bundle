{
  "name": "mediamonks/crawler-bundle",
  "type": "symfony-bundle",
  "description": "MediaMonks Crawler Bundle",
  "keywords": [
    "crawler",
    "seo",
    "dom",
    "spider",
    "robot",
    "prerender",
    "prerender.io"
  ],
  "homepage": "https://www.mediamonks.com/",
  "license": "MIT",
  "authors": [
    {
      "name": "Robert Slootjes",
      "email": "robert@mediamonks.com",
      "homepage": "https://github.com/slootjes"
    }
  ],
  "require": {
    "php": "^5.5|~7.0",
    "mediamonks/crawler": "^1.0",
    "symfony/framework-bundle": "^2.8|^3.0"
  },
  "require-dev": {
    "codeclimate/php-test-reporter": "dev-master@dev",
    "matthiasnoback/symfony-dependency-injection-test": "0.*",
    "phpunit/phpunit": "^4.8",
    "mockery/mockery": "^0.9.4"
  },
  "autoload": {
    "psr-4": {
      "MediaMonks\\CrawlerBundle\\": ""
    }
  },
  "autoload-dev": {
    "psr-0": {
      "": "Tests/Functional/src/"
    }
  },
  "extra": {
    "branch-alias": {
      "dev-master": "1.0-dev"
    }
  }
}
